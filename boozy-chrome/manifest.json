{
    "manifest_version": 2,

    "name": "Boozy Browser",
    "description": "Please drink responsibly...",
    "version": "0.0",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "css": ["/css/utils.css", "/css/boozychrome.css"],
            "js": ["/js/lib/jquery-2.1.0.min.js", "/js/lib/underscore-min.js", "/js/boozybrowser.js","/js/boozychrome.js"],
            "run_at": "document_end"
        }
    ],
    "permissions": [],
    "browser_action": {
        "default_popup": "/html/popup.html"
    }
}
